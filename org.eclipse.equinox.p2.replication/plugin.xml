<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.exportWizards">
      <category
            id="org.eclipse.equinox.p2.category"
            name="%category.name">
      </category>
      <wizard
            category="org.eclipse.equinox.p2.category"
            class="org.eclipse.equinox.p2.replication.internal.wizard.ExportWizard"
            icon="icons/ico_import_export.gif"
            id="org.eclipse.equinox.p2.replication.export"
            name="%export.name">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.importWizards">
      <category
            id="org.eclipse.equinox.p2.category"
            name="%category.name">
      </category>
      <wizard
            category="org.eclipse.equinox.p2.category"
            class="org.eclipse.equinox.p2.replication.internal.wizard.ImportWizard"
            icon="icons/ico_import_export.gif"
            id="org.eclipse.equinox.p2.replication.import"
            name="%import.name">
      </wizard>
      <wizard
            category="org.eclipse.equinox.p2.category"
            class="org.eclipse.equinox.p2.replication.internal.wizard.ImportFromInstallationWizard"
            icon="icons/ico_import_export.gif"
            id="org.eclipse.equinox.p2.replication.importfrominstallation"
            name="%importfrominstallation.name">
      </wizard>
   </extension>

</plugin>
